<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="org.hbird.rcpgui.hummingbirdbaseapp.application"
         point="org.eclipse.core.runtime.applications">
      <application
            icon="icons/transmit_blue.png">
         <run
               class="org.hbird.rcpgui.hummingbirdbaseapp.HummingbirdCoreApplication">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.hbird.rcpgui.hummingbirdbaseapp.perspectives.McsHomePerspective"
            id="org.hbird.rcpgui.hummingbirdbaseapp.perspectives.McsHomePerspective"
            name="%perspective.name">
      </perspective>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.hbird.rcpgui.hummingbirdbaseapp.application"
            name="Hummingbird - Mission Control System">
         <property
               name="windowImages"
               value="icons/alt_window_16.gif,icons/alt_window_32.gif">
         </property>
         <property
               name="appName"
               value="Hummingbird - Mission Control System">
         </property>
         <property
               name="startupProgressRect"
               value="5,5,445,15">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupMessageRect"
               value="7,320,445,20">
         </property>
         <property
               name="aboutText"
               value="Hummingbird Mission Control System&#x0A;&#x0A;Johannes Klug - Gert Villemos - Mark Doyle">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.views">
   </extension>
   <extension
         point="org.eclipse.ui.intro">
      <intro
            class="org.eclipse.ui.intro.config.CustomizableIntroPart"
            id="org.hbird.rcpgui.hummingbirdbaseapp.intro">
      </intro>
      <introProductBinding
            introId="org.hbird.rcpgui.hummingbirdbaseapp.intro"
            productId="org.hbird.rcpgui.hummingbirdbaseapp.product">
      </introProductBinding>
   </extension>
   <extension
         point="org.eclipse.ui.intro.config">
      <config
            content="introContent.xml"
            id="org.hbird.rcpgui.hummingbirdbaseapp.introConfigId"
            introId="org.hbird.rcpgui.hummingbirdbaseapp.intro">
         <presentation
               home-page-id="root">
            <implementation
                  kind="html"
                  os="win32,linux,macosx"
                  style="content/shared.css">
            </implementation>
         </presentation>
      </config>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?">
         <menu
               id="org.hbird.rcpgui.hummingbirdbaseapp.menus.file"
               label="%menu.label"
               mnemonic="%menu.mnemonic">
            <command
                  commandId="org.hbird.rcpgui.hummingbirdbaseapp.commands.exit"
                  label="%command.label"
                  mnemonic="%command.mnemonic"
                  style="push"
                  tooltip="%command.tooltip">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.hbird.rcpgui.hummingbirdbaseapp.commands.exit"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
   </extension>

</plugin>
